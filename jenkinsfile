#!groovy
@Library('jenkinslib') _
def tools = new org.devops.tools()

pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                script {
                tools. PrintMes('==========1th stages 打印蓝色==========','blue')
                }
            }
        }
       
    }
}
